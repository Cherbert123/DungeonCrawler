package lootPackage;

import java.util.Random;

public class Player {
	Random random;
	int numOne;
	int numTwo;
	int numThree;
	int numFour;
	int numFive;
	public Player(){
	}
	public void playerCreate(){
		numOne = random.nextInt(12) + 9;
		numTwo = random.nextInt(19) + 1;
		numThree = random.nextInt(19) + 1;
		numFour = random.nextInt(10) + 11;
		numFive = random.nextInt(13) + 8;
		System.out.println(numOne + numTwo + numThree + numFour + numFive);
	}
}
